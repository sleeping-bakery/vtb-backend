{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=postgres_app;Database=${POSTGRES_DATABASE};User Id=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};"
  },
  "KeycloakSettings": {
    "Authority": "${KEYCLOAK_URL}",
    "ValidAudience": "${KEYCLOAK_AUDIENCE}",
    "ValidIssuer": "${KEYCLOAK_URL}/realms/${KEYCLOAK_REALM}",
    "CertificateUrl": "http://keycloak:8080/realms/${KEYCLOAK_REALM}/protocol/openid-connect/certs"
  },
  "MockOpenApiBankClientsSettings": {
    "IsAccountClientMock": "${IS_ACCOUNT_CLIENT_MOCK}",
    "IsCardClientMock": "${IS_CARD_CLIENT_MOCK}",
    "IsUnidentifiedPaymentClientMock": "${IS_UNIDENTIFIED_PAYMENT_CLIENT_MOCK}",
    "IsServiceClientMock": "${IS_SERVICE_CLIENT_MOCK}",
    "IsUniversalPaymentClientMock": "${IS_UNIVERSAL_PAYMENT_CLIENT_MOCK}",
    "IsPeriodPaymentClientMock": "${IS_PERIOD_PAYMENT_CLIENT_MOCK}",
    "IsBonusPointClientMock": "${IS_BONUS_POINT_CLIENT_MOCK}",
    "IsGuaranteeClientMock": "${IS_GUARANTEE_CLIENT_MOCK}"
  }
}
